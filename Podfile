# Uncomment the next line to define a global platform for your project
# platform :ios, '12.0'

target 'fcst_example' do
  # 앱에서 사용할 Pods
  pod 'Alamofire', '~> 5.6'
  pod 'ObjectMapper', '~> 4.2'

  # Unit tests용 타겟 (필요한 경우 여기에 테스트 전용 Pod 추가)
  target 'fcst_exampleTests' do
    inherit! :search_paths
    # pod 'SomeTestingPod'
  end

  # UI tests용 타겟 (필요한 경우 여기에 테스트 전용 Pod 추가)
  target 'fcst_exampleUITests' do
    inherit! :search_paths
    # pod 'SomeUITestingPod'
  end
end

